    -webkit-transform: rotate(-30deg);
    animation: indexCircleRight 6s linear infinite;
    -webkit-animation: indexCircleRight 6s linear infinite;
    -moz-animation: indexCircleRight 6s linear infinite;
}
@keyframes indexCircleRight {
	  0%{ margin-top: -340px;}	 
	 50%{ margin-top: -280px;}	 
	100%{ margin-top: -340px;}	 	
}
@-moz-keyframes indexCircleRight {
	  0%{ margin-top: -340px;}	 
