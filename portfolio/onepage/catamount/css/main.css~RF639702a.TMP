.circle_yellow {
    position: absolute;
    width: 280px;
    left: -135px;
    top: 50%;
    margin-top: -62px;
    z-index: 0;
    transform: rotate(-70deg);
    -moz-transform: rotate(-70deg);
    -webkit-transform: rotate(-70deg);
     animation: indexCircleLeft 8s linear infinite;
    -webkit-animation: indexCircleLeft 8s linear infinite;
    -moz-animation: indexCircleLeft 8s linear infinite;
}
@keyframes indexCircleLeft {
	  0%{ margin-top: -62px;}	 
	 50%{ margin-top: -152px;}	 
	100%{ margin-top: -62px;}				
}
@-moz-keyframes indexCircleLeft {
	  0%{ margin-top: -62px;}	 
	 50%{ margin-top: -152px;}	 
	100%{ margin-top: -62px;}				
}
@-webkit-keyframes indexCircleLeft {
	  0%{ margin-top: -62px;}	 
	 50%{ margin-top: -152px;}	 
	100%{ margin-top: -62px;}			 	
}
.circle_white {
    position: absolute;
    width: 220px;
    right: -90px;
